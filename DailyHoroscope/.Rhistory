snr<-function(x){
en<-floor(x/10)
tn<-(x/10-floor(x/10))*10
x<-en+tn
return(x)
}
yn<-function(x){
y1<-floor(x/1000)
y2<-floor((x-y1*1000)/100)
y3<-floor((x-y1*1000-y2*100)/10)
y4<-x-y1*1000-y2*100-y3*10
y<-y1+y2+y3+y4
y<-snr(y)
while(nchar(y)>1){
y<-snr(y)
}
return(y)
}
